{
    "name": "artisan-build/community-prompts",
    "type": "library",
    "description": "Beautiful and user-friendly forms for your command-line PHP applications built by the community for the Laravel/Prompts package.",
    "license": "MIT",
    "authors": [
        {
            "name": "Len Woodward",
            "email": "Len@ProjektGopher.com",
            "homepage": "https://LenWoodward.com",
            "role": "Maintainer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ArtisanBuild\\CommunityPrompts\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "react/stream": "^1.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11",
        "pestphp/pest": "^2.3",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan-mockery": "^1.1",
        "laravel/prompts": "^0.3.0"
    },
    "suggest": {
        "laravel/prompts": "Required for the abstract Prompt class."
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
